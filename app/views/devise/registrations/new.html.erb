<div class="container">
  <div class="row">
    <div class="col-md-4 col-md-offset-4">
      <div>
        <% if params[:plan] == '2' %>
          <h2 class="text-center">DevLink Pro</h2>
          <p class="text-center">Sign up for DevLink Pro and get access to the community, contact information, and more.</p>
        <% else %>
          <h2 class="text-center">DevLink Basic</h2>
          <p class="text-center">Sign up for DevLink Basic and gain instant access to the community.</p>
        <% end %>
      </div>
      <div class="well">
        <% if params[:plan] == '2' %>
          <%= render "pro_form" %>
        <% else %>
          <%= render "basic_form" %>
        <% end %>
        <%= render "devise/shared/links" %>
      </div>
    </div>
  </div>
</div>